{
  "name": "@intlify/vue2-i18n-jest",
  "description": "@vue/vue2-jest transformer for i18n custom blocks",
  "version": "26.0.0",
  "author": {
    "name": "kazuya kawaguchi",
    "email": "kawakazu80@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/intlify/vue-i18n-jest/issues"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "js-yaml": "^3.13.1",
    "json5": "^2.1.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/jest": "^24.0.0",
    "@types/js-yaml": "^3.12.1",
    "@types/json5": "^0.0.30",
    "@types/node": "^13.13.0",
    "@types/source-map": "^0.5.7",
    "jest": "^24.0.0",
    "lerna": "^4.0.0",
    "lerna-changelog": "^1.0.1",
    "opener": "^1.5.1",
    "shipjs": "^0.23.0",
    "ts-jest": "^24.0.0",
    "typescript": "^3.8.3",
    "vue": "^2.5",
    "vue-template-compiler": "^2.5"
  },
  "engines": {
    "node": ">= 12"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/intlify/vue-i18n-jest#readme",
  "keywords": [
    "i18n",
    "jest",
    "vue",
    "vue-i18n"
  ],
  "license": "MIT",
  "main": "./lib/index.js",
  "peerDependencies": {
    "vue": "^2.5",
    "vue-jest": "^4.0.0-beta.3",
    "vue-template-compiler": "^2.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/intlify/vue-i18n-jest.git"
  },
  "scripts": {
    "build": "tsc -p .",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -n ./node_modules/git-commit-message-convention/convention.js",
    "clean": "rm -rf ./coverage && rm -rf ./lib/*.js*",
    "coverage": "opener coverage/lcov-report/index.html",
    "lint": "eslint ./src ./test --ext .ts",
    "release:prepare": "shipjs prepare",
    "release:trigger": "shipjs trigger",
    "test": "npm run lint && npm run test:cover",
    "test:cover": "npm run test:unit -- --coverage",
    "test:e2e": "yarn build && yarn --cwd ./e2e install && yarn --cwd ./e2e test",
    "test:unit": "jest --env node",
    "watch": "tsc -p . --watch"
  }
}
